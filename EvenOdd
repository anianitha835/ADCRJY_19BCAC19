import java.util.*;
public class EvenOdd {

	public static void main(String[] args) {
		System.out.println("enter a number:");
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		if((num & 1)!=1) {
			System.out.println(num +" is an even");
		}
		else {
			System.out.println(num +" is an odd");
		}
		
	}

}
