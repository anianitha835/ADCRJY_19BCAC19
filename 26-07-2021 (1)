class Solution {
    public int countPrimes(int n) {
        int count=0;
        for(int i=2;i<n;i++){
           if (isPrime(i, 2)) {
            count++;
        }
    }

    return count;
}

public boolean isPrime(int num, int x) {
    if (num % x != 0 || x == num) {
        if (x > Math.sqrt(num))
            return true;

        return isPrime(num, ++x);
    }

    return false;

    }      

public static void main(String args[]){
    Solution s=new Solution();
    s.countPrimes(int n);
    }
  }
