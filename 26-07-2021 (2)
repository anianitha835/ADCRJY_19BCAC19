class Solution {
    public int trailingZeroes(int n) {
        int count = 0;
         while(n/5 != 0) {
           count += n/5;;
             n /= 5;
        }
        return count;
        
    }
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int n=sc.mextInt();
Solution s=new Solution();
s.trailingZeroes(n);
}
}
