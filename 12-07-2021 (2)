import java.util.*;
public class ReverseStr {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String str=sc.next();
		String[] words=str.split("\\.");
		int i=0;
		for(i=words.length-1;i>0;i--) 
		{
			System.out.print(words[i]+".");
		}
		System.out.println(words[i]);
}
}
