public class Factorial {

	public static void main(String[] args) {
		{
			Scanner sc=new Scanner(System.in);
		    int t=sc.nextInt();
		    
		    for(int i=1;i<=t;i++){
		        int n=sc.nextInt();
		        
		        int fact=1;
		        for(int j=n;j>=1;j--){
		            fact*=j;
		          }
		        System.out.println(fact);
		    }
		    t--;
		}
	}
}


output:
3

4
24

6
720

5
120
