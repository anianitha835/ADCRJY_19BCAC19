import java.util.*;

public class VowelOrConsonant {
	
	public static void main(String ar[])
	{
		Scanner ch=new Scanner(System.in);
		String c=ch.nextLine();
		switch(c)
		{
		case "a":
		case "e":
		case "i":
		case "o":
		case "u":
			System.out.println(c +" is a vowel");
			break;
		default:
			System.out.println(c +" is a consonant");
			
		
		}
	}
}
